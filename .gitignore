# Ignore Node.js dependencies and build files
node_modules/
.yarn/
.pnp/
.pnp.*
yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# Ignore Next.js build and cache files
.next/           # Next.js build files
/out/            # Static export files generated by Next.js
/build/          # General build files

# Ignore testing coverage
coverage/        # Testing coverage reports

# Ignore Python virtual environments and compiled files (if you are working with a Python backend)
venv/
.env/
*.pyc
__pycache__/

# Ignore IDE/editor files (e.g., VSCode, JetBrains, etc.)
.vscode/
.idea/

# Ignore OS-specific files
.DS_Store

# Ignore debug logs and npm-related files
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# Ignore environment variables (can opt-in for committing if needed)
.env*

# Ignore Vercel files (for deployment)
.vercel

# Ignore TypeScript build information (for Next.js or other TS projects)
*.tsbuildinfo
next-env.d.ts

# Miscellaneous
*.pem
config.json

# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.
